{
  "appId": "com.jeudedames.app",
  "appName": "Jeu de Dames",
  "webDir": "www",
  "server": {
    "androidScheme": "https"
  },
  "android": {
    "backgroundColor": "#1a1a2e",
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": false
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#1a1a2e",
      "showSpinner": false,
      "androidSpinnerStyle": "small",
      "spinnerColor": "#4fc3f7"
    }
  }
}
